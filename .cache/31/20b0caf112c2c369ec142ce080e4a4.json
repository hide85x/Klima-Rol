inal":{"line":208,"column":0},"generated":{"line":1,"column":2689}},{"source":"style.scss","name":null,"original":{"line":211,"column":0},"generated":{"line":1,"column":2730}},{"source":"style.scss","name":null,"original":{"line":212,"column":4},"generated":{"line":1,"column":2749}}],"sources":{"style.scss":"@import url(https://cdn.rawgit.com/tonsky/FiraCode/1.205/distr/fira_code.css);\r\n\r\n\r\nhtml {\r\n    box-sizing: border-box;\r\n  }\r\na {\r\n    color: inherit;\r\n    width: 100%;\r\n    text-decoration: none\r\n\r\n}\r\n*, *::before, *::after {\r\n    -webkit-font-smoothing: antialiased;\r\n    -moz-osx-font-smoothing: grayscale;\r\n\r\n    box-sizing: border-box;\r\n    -webkit-font-smoothing: antialiased;\r\n    -moz-osx-font-smoothing: grayscale;\r\n  }\r\n\r\nbody {\r\n    display: flex;\r\n    flex-direction: column;\r\n    font-family: 'Fira Code', sans-serif;\r\n    background-image: linear-gradient(rgba(255, 123, 0, 0.637), rgba(255, 166, 0, 0.719), rgba(255, 255, 0, 0.5));\r\n    ;\r\n}\r\n.nav {\r\n    border:2px solid black;\r\n    // border-radius: 0 20px 0 20px;\r\n    display: flex;\r\n    margin-bottom: 100px;\r\n    background-color: rgb(255, 166, 0);\r\n    .navbar {\r\n        display: flex;\r\n        justify-content: space-evenly;\r\n        width: 100%;\r\n        padding: 5px;\r\n        &__item {\r\n            margin: 0;\r\n            list-style-type: none\r\n        }\r\n    }\r\n}\r\n\r\n.main {\r\n    display: flex;\r\n    flex-direction: column;\r\n    flex-wrap: wrap;\r\n\r\n\r\n    .hero {\r\n        margin: auto;\r\n        margin-bottom: 100px;\r\n        text-align: center;\r\n        border-top: 3px solid black;\r\n        // border-bottom: 2px solid black;\r\n        padding:100px 100px 0 100px;\r\n        background-color: #fff;\r\n\r\n        // background-image: linear-gradient(rgb(255, 124, 2),orange,  rgb(255, 255, 255));\r\n    }\r\n    &__row {\r\n        background-color: #fff;\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        margin: 50px 0;\r\n        box-shadow: 0 4px 16px -2px gray;\r\n        // mozna dodac padding dla duzych monitorow\r\n        \r\n\r\n    \r\n        img {\r\n            max-width: 40%;\r\n            min-width: 300px;\r\n        }\r\n        &__content {\r\n            \r\n            display: flex;\r\n            flex-direction: column;\r\n            text-align: left;\r\n            margin: auto;\r\n            padding: 20px 100px;\r\n            width: 60%;\r\n            font-size: 1.6rem;\r\n        }\r\n        &__i {\r\n            margin: auto;\r\n            i {\r\n                color: rgb(31, 153, 0);\r\n                text-shadow: 9px 9px 1px rgba(0, 0, 0, 0.753);\r\n    \r\n                    padding: 60px;\r\n                    opacity: 0.9;\r\n                }\r\n        }\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n//hamburger \r\n\r\n\r\n.sidebarnav {\r\n    z-index: 999;\r\n    display: flex;\r\n    align-self: center;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    text-align: center;\r\n    position: fixed;\r\n    width: 300px;\r\n    margin-top: 300px;\r\n    transform: translateY(-1000px);\r\n    border-radius: 100%;\r\n    transition:transform 0.5s ease-in, background-color 1s ease-in;\r\n    @media (max-width:800px) {\r\n        margin-top:35px\r\n    }\r\n    a, i {\r\n        text-decoration: none;\r\n        font-size: 2em;\r\n        color: white;\r\n        margin:10px;\r\n        @media(max-width:700px) {\r\n            margin: 2px;\r\n            font-size: 1.2em;\r\n            color: red($color: #000000);\r\n        }\r\n        &:hover {\r\n            color: rgb(255, 153, 0);\r\n        }\r\n    }\r\n}\r\n.sidebarnav--active {\r\n    transform: translateY(-50px);\r\n    background-color: rgba(0, 0, 0, 0.836);\r\n\r\n}\r\n\r\n\r\n\r\n.hamburger {\r\n    padding: 10px;\r\n    display: inline-block;\r\n    cursor: pointer;\r\n    background-color: transparent;\r\n    border:0;\r\n    margin: 0\r\n}\r\n.hamburger__box {\r\n    width:40px;\r\n    height: 24px;\r\n    display: inline-block;\r\n    position: relative;\r\n}\r\n@mixin hamburger-line {\r\n    width:100%;\r\n    height: 3px;\r\n    position: absolute;\r\n    background-color: black\r\n}\r\n.hamburger__inner {\r\n    @include hamburger-line;\r\n    \r\n        left: 0;\r\n        top: 50%;\r\n        transform: translateY(-50%);\r\n        transition: background-color 0.3s .1s ease-in-out\r\n}\r\n\r\n.hamburger__inner::before,\r\n.hamburger__inner::after\r\n {\r\n    @include hamburger-line;\r\n    content: '';\r\n    left: 0;\r\n    transition: transform 0.3s ease-in-out\r\n}\r\n\r\n.hamburger__inner::after {\r\n\r\n    top: 10px;\r\n}\r\n\r\n.hamburger__inner::before {\r\n\r\n    top: -10px;\r\n}\r\n\r\n.hamburger--active  .hamburger__inner {\r\n    background-color: transparent\r\n}\r\n.hamburger--active .hamburger__inner::before {\r\n     transform:translateY(10px)  rotate(45deg) \r\n\r\n}\r\n.hamburger--active .hamburger__inner::after{\r\n    transform:translateY(-10px) rotate(-45deg) \r\n\r\n}\r\n.hamburger, .nav {\r\ntransition: transform 0.3s 0.1s ease-in-out\r\n\r\n}\r\n.hamburger--active {\r\n    transform: translate(0px)\r\n}"},"lineCount":null}},"error":null,"hash":"8e9039a7495ee46c0896f0ab547d819e","cacheData":{"env":{}}}